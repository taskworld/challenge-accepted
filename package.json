{
  "name": "challenge-accepted",
  "version": "1.0.0",
  "description": "Composable testing framework.",
  "dependencies": {
    "bluebird": "^3.3.1",
    "chalk": "^1.1.1",
    "minimist": "^1.2.0",
    "ms": "^0.7.1",
    "tap": "^5.7.0"
  },
  "bin": {
    "challenge-accepted": "./cli.js"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-plugin-__coverage__": "^0.111.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "bulk-require": "^0.2.1",
    "codecov": "^1.0.1",
    "concurrently": "^2.0.0",
    "nyc": "^5.6.0",
    "tap-mocha-reporter": "0.0.24"
  },
  "scripts": {
    "build": "ruby build.rb && babel src -d lib",
    "clean": "rm -rf src lib",
    "dev": "concurrently 'onchange README.md -- ruby build.rb' 'babel src -d lib -w'",
    "prebuild": "npm run clean",
    "pretest": "env NODE_ENV=test npm run build",
    "tap": "babel-node src/cli.js src/test/index.js | tap-mocha-reporter spec",
    "test": "nyc -r lcov -r text -- node lib/cli.js lib/test/index.js | tap-mocha-reporter spec"
  },
  "nyc": {
    "include": [
      "/"
    ]
  },
  "author": "Thai Pangsakulyanont @ Taskworld <thai.p@taskworld.com>",
  "license": "MIT"
}
